arg msg .

address ISPEXEC

"CONTROL ERRORS RETURN"
"GETMSG MSG("msg") SHORTMSG(A) LONGMSG(B) TYPE(C) ALARM(D) HELP(E)"
if RC > 0 then
  do
    "RDISPLAY Message" msg "Not Found"
    exit
  end

"RDISPLAY SMSG:" A
"RDISPLAY LMSG:" B
"RDISPLAY TYPE:" C
"RDISPLAY ALRM:" D
"RDISPLAY HELP:" E

exit
