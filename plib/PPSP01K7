)PANEL VERSION=1 FORMAT=1

)COMMENT
 Confirm delete panel
)ENDCOMMENT

)BODY WINDOW(60,11)
PanelTitle "Confirm Delete of &ZZENTRY"

text     2   2       FP  "Command ===> "
field    2  15   MAX NEF CAPS(ON),PAD(USER) ZCMD

text     4   2  PIN  "Enter / to confirm delete"
field    4   28   1   CEF PAD(USER) ENTRYDEL

text     6 2  CH "Instructions"
text     8 2  NT "Press"
text     8 8  ET "Enter"
text     8 14 NT "to confirm delete"

text     9 2  NT "Press"
text     9 8  ET "CANCEL"
text     9 15 NT "or"
text     9 18 ET "END"
text     9 22 NT "to cancel delete."

)INIT

.CURSOR   = ENTRYDEL
&ENTRYDEL = &Z
&ZWINTTL  = &Z
&ZCMD     = &Z

)PROC
IF (&ZCMD NE &Z)
    .MSG    = PSYS018
    .CURSOR = ZCMD
    EXIT

VER (&ENTRYDEL NB,LIST,/)

)END
