)PANEL VERSION=1 FORMAT=1
)COMMENT
 Panel for Issuing commands
)ENDCOMMENT

)BODY CMD(ZCMD)

PanelTitle "Execute Command"

literal   4   2       FP "Command ===> "
field     4  15  MAX  NEF CAPS(ON),PAD(USER) ZCMD

literal   6  2        FP  "EXECUTE ===> "
field     6  15  MAX  NEF CAPS(OFF),PAD(USER) ZCOMMAND

)INCLUDE STD_PDC
)INCLUDE HLP_PDC


)INIT
 .cursor = zcommand

)REINIT

)PROC

IF ( &ZCOMMAND EQ &Z )
  .CURSOR = ZCOMMAND
  EXIT


)END
