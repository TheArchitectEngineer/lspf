)PANEL VERSION=1 FORMAT=1
)COMMENT
 Panel for Issuing commands
)ENDCOMMENT

)BODY CMD(ZCMD)

PanelTitle "Execute Command"

literal   4  2        CH  "Enter command below"

literal   6  2        FP  "===>"
field     6  7   MAX  NEF CAPS(OFF),PAD(USER) ZCMD

literal   8   2       NT  "Place cursor on choice and press enter to &TYPE command"
literal  10   2       FP  "=>"
field    10   5  MAX NEF CAPS(OFF),PAD(USER) ZCMD1
literal  11   2       FP  "=>"
field    11   5  MAX NEF CAPS(OFF),PAD(USER) ZCMD2
literal  12   2       FP  "=>"
field    12   5  MAX NEF CAPS(OFF),PAD(USER) ZCMD3
literal  13   2       FP  "=>"
field    13   5  MAX NEF CAPS(OFF),PAD(USER) ZCMD4
literal  14   2       FP  "=>"
field    14   5  MAX NEF CAPS(OFF),PAD(USER) ZCMD5
literal  15   2       FP  "=>"
field    15   5  MAX NEF CAPS(OFF),PAD(USER) ZCMD6
literal  16   2       FP  "=>"
field    16   5  MAX NEF CAPS(OFF),PAD(USER) ZCMD7
literal  17   2       FP  "=>"
field    17   5  MAX NEF CAPS(OFF),PAD(USER) ZCMD8
literal  18   2       FP  "=>"
field    18   5  MAX NEF CAPS(OFF),PAD(USER) ZCMD9
literal  19   2       FP  "=>"
field    19   5  MAX NEF CAPS(OFF),PAD(USER) ZCMD10
literal  20   2       FP  "=>"
field    20   5  MAX NEF CAPS(OFF),PAD(USER) ZCMD11
literal  21   2       FP  "=>"
field    21   5  MAX NEF CAPS(OFF),PAD(USER) ZCMD12

)INCLUDE STD_PDC

pdc  List  "Update On"   ACTION RUN(>CMDUPN)
pdc  List  "Update Off"  ACTION RUN(>CMDUPF)

pdc  Mode  "Retrieve"  ACTION RUN(>CMDRET)
pdc  Mode  "Execute"   ACTION RUN(>CMDEXE)
pdc  Mode  "Delete"    ACTION RUN(>CMDDEL)

)INCLUDE HLP_PDC

)INIT
.CURSOR = ZCMD

VGET (LIST,MODE,ZCMD1,ZCMD2,ZCMD3,ZCMD4,ZCMD5,ZCMD6) PROFILE
VGET (ZCMD7,ZCMD8,ZCMD9,ZCMD10,ZCMD11,ZCMD12) PROFILE

IF (&LIST EQ &Z) &LIST = ON
IF (&MODE EQ &Z) &MODE = CMDRET

IF (&MODE EQ CMDRET) &TYPE = 'retreive'
ELSE
  IF (&MODE EQ CMDEXE) &TYPE = 'execute'
  ELSE                 &TYPE = 'delete'

)REINIT
REFRESH(*)

)PROC

IF (&ZCMD EQ CMDRET,CMDEXE,CMDDEL)
   &MODE = &ZCMD
   &ZCMD = &Z
   VPUT MODE PROFILE

IF (&ZCMD EQ CMDUPN)
   &LIST = ON
   &ZCMD = &Z
   VPUT LIST PROFILE
ELSE
  IF (&ZCMD EQ CMDUPF)
     &LIST = OFF
     &ZCMD = &Z
     VPUT LIST PROFILE

IF (.CURSOR EQ ZCMD1)
   IF (&MODE EQ CMDDEL)
      &ZCMD1 = &Z
      VPUT ZCMD1 PROFILE
      EXIT
   &ZCMD = &ZCMD1
   IF (&MODE EQ CMDRET)
      .MSG = PSYS011V
      EXIT
ELSE
   IF (.CURSOR EQ ZCMD2)
      IF (&MODE EQ CMDDEL)
         &ZCMD2 = &Z
         VPUT ZCMD2 PROFILE
         EXIT
      &ZCMD = &ZCMD2
      IF (&MODE EQ CMDRET)
         .MSG = PSYS011V
         EXIT
   ELSE
      IF (.CURSOR EQ ZCMD3)
         IF (&MODE EQ CMDDEL)
            &ZCMD3 = &Z
            VPUT ZCMD3 PROFILE
            EXIT
         &ZCMD = &ZCMD3
         IF (&MODE EQ CMDRET)
            .MSG = PSYS011V
            EXIT
      ELSE
         IF (.CURSOR EQ ZCMD4)
            IF (&MODE EQ CMDDEL)
               &ZCMD4 = &Z
               VPUT ZCMD4 PROFILE
               EXIT
            &ZCMD = &ZCMD4
            IF (&MODE EQ CMDRET)
               .MSG = PSYS011V
               EXIT
         ELSE
            IF (.CURSOR EQ ZCMD5)
               IF (&MODE EQ CMDDEL)
                  &ZCMD5 = &Z
                  VPUT ZCMD5 PROFILE
                  EXIT
               &ZCMD = &ZCMD5
               IF (&MODE EQ CMDRET)
                  .MSG = PSYS011V
                  EXIT
            ELSE
               IF (.CURSOR EQ ZCMD6)
                  IF (&MODE EQ CMDDEL)
                     &ZCMD6 = &Z
                     VPUT ZCMD6 PROFILE
                     EXIT
                  &ZCMD = &ZCMD6
                  IF (&MODE EQ CMDRET)
                     .MSG = PSYS011V
                     EXIT
               ELSE
                  IF (.CURSOR EQ ZCMD7)
                     IF (&MODE EQ CMDDEL)
                        &ZCMD7 = &Z
                        VPUT ZCMD7 PROFILE
                        EXIT
                     &ZCMD = &ZCMD7
                     IF (&MODE EQ CMDRET)
                        .MSG = PSYS011V
                        EXIT
                  ELSE
                     IF (.CURSOR EQ ZCMD8)
                        IF (&MODE EQ CMDDEL)
                           &ZCMD8 = &Z
                           VPUT ZCMD8 PROFILE
                           EXIT
                        &ZCMD = &ZCMD8
                        IF (&MODE EQ CMDRET)
                           .MSG = PSYS011V
                           EXIT
                     ELSE
                        IF (.CURSOR EQ ZCMD9)
                           IF (&MODE EQ CMDDEL)
                              &ZCMD9 = &Z
                              VPUT ZCMD9 PROFILE
                              EXIT
                           &ZCMD = &ZCMD9
                           IF (&MODE EQ CMDRET)
                              .MSG = PSYS011V
                              EXIT
                        ELSE
                           IF (.CURSOR EQ ZCMD10)
                              IF (&MODE EQ CMDDEL)
                                 &ZCMD10 = &Z
                                 VPUT ZCMD10 PROFILE
                                 EXIT
                              &ZCMD = &ZCMD10
                              IF (&MODE EQ CMDRET)
                                 .MSG = PSYS011V
                                 EXIT
                           ELSE
                              IF (.CURSOR EQ ZCMD11)
                                 IF (&MODE EQ CMDDEL)
                                    &ZCMD11 = &Z
                                    VPUT ZCMD11 PROFILE
                                    EXIT
                                 &ZCMD = &ZCMD11
                                 IF (&MODE EQ CMDRET)
                                    .MSG = PSYS011V
                                    EXIT
                              ELSE
                                 IF (.CURSOR EQ ZCMD12)
                                    IF (&MODE EQ CMDDEL)
                                       &ZCMD12 = &Z
                                       VPUT ZCMD12 PROFILE
                                       EXIT
                                    &ZCMD = &ZCMD12
                                    IF (&MODE EQ CMDRET)
                                       .MSG = PSYS011V
                                       EXIT

IF (&LIST EQ ON AND &ZCMD NE &Z AND &ZCMD1 NE &ZCMD)
   IF (&ZCMD11 NE &Z) &ZCMD12 = &ZCMD11
   IF (&ZCMD10 NE &Z) &ZCMD11 = &ZCMD10
   IF (&ZCMD9  NE &Z) &ZCMD10 = &ZCMD9
   IF (&ZCMD8  NE &Z) &ZCMD9  = &ZCMD8
   IF (&ZCMD7  NE &Z) &ZCMD8  = &ZCMD7
   IF (&ZCMD6  NE &Z) &ZCMD7  = &ZCMD6
   IF (&ZCMD5  NE &Z) &ZCMD6  = &ZCMD5
   IF (&ZCMD4  NE &Z) &ZCMD5  = &ZCMD4
   IF (&ZCMD3  NE &Z) &ZCMD4  = &ZCMD3
   IF (&ZCMD2  NE &Z) &ZCMD3  = &ZCMD2
   IF (&ZCMD1  NE &Z) &ZCMD2  = &ZCMD1
   &ZCMD1 = &ZCMD

VPUT (ZCMD1,ZCMD2,ZCMD3,ZCMD4,ZCMD5,ZCMD6) PROFILE
VPUT (ZCMD7,ZCMD8,ZCMD9,ZCMD10,ZCMD11,ZCMD12) PROFILE

IF ( &ZCMD EQ &Z )
  .CURSOR = ZCMD
  EXIT

)END
