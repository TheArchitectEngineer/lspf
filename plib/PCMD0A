)PANEL VERSION=1 FORMAT=1
)COMMENT
 Panel for Issuing commands
)ENDCOMMENT

)BODY

 PanelTitle "Execute Command" 
 
 literal   4   2       FP "COMMAND ===> "   
 field     4  15  MAX  NEF CAPS(ON) ZCMD   

 literal   6  2        FP  "EXECUTE ===> "   
 field     6  15  MAX  NEF CAPS(OFF) ZCOMMAND   

)INCLUDE STD_PDC
)INCLUDE HLP_PDC

)ENDBODY

)INIT
 .cursor = zcommand

)ENDINIT


)REINIT
)ENDREINIT

)PROC

IF ( &ZCOMMAND EQ &Z )  
  .CURSOR = ZCOMMAND   
  EXIT

)ENDPROC
