box  10 2 82 15 B_YELLOW "TO DO List"

)INCLUDE STD_PDC
)INCLUDE HLP_PDC


)INIT
&ZCMD = &Z

)PROC

&ZCMD = TRANS (&ZCMD CAN,CANCEL *,* )

IF (&ZCMD = CANCEL ) EXIT

IF (.RESP = END )
    vput ( TODO1 TODO2  TODO3  TODO4  TODO5 TODO6 TODO7 TODO8 ) PROFILE
    vput ( TODO9 TODO10 TODO11 TODO12 ) PROFILE

)END
