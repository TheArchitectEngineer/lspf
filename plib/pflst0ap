FIELD 16  10  MAX-4 NEF PAD(USER) PFLCMD15
FIELD 16  MAX-2     2   LI NONE   PFLC15IN

)INIT
&ZCMD = &Z
VGET (PFLENT00,PFLCMD00) PROFILE
VGET (PFLENT01,PFLCMD01) PROFILE
VGET (PFLENT02,PFLCMD02) PROFILE
VGET (PFLENT03,PFLCMD03) PROFILE
VGET (PFLENT04,PFLCMD04) PROFILE
VGET (PFLENT05,PFLCMD05) PROFILE
VGET (PFLENT06,PFLCMD06) PROFILE
VGET (PFLENT07,PFLCMD07) PROFILE
VGET (PFLENT08,PFLCMD08) PROFILE
VGET (PFLENT09,PFLCMD09) PROFILE
VGET (PFLENT10,PFLCMD10) PROFILE
VGET (PFLENT11,PFLCMD11) PROFILE
VGET (PFLENT12,PFLCMD12) PROFILE
VGET (PFLENT13,PFLCMD13) PROFILE
VGET (PFLENT14,PFLCMD14) PROFILE
VGET (PFLENT15,PFLCMD15) PROFILE

)PROC
IF (&ZCMD NE &Z)
   .MSG    = PSYS011C
   .CURSOR = ZCMD
   EXIT

&AAA = TRUNC(&PFLENT00,' ')
IF (.TRAIL NE &Z)
   .MSG    = FLST013D
   .CURSOR = PFLENT00
   EXIT

IF (&PFLENT00 NE &Z AND &PFLCMD00 EQ &Z)
   &PFLCMD00 = 'SETPATH &ZPATH'

&AAA = TRUNC(&PFLENT01,' ')
IF (.TRAIL NE &Z)
   .MSG    = FLST013D
   .CURSOR = PFLENT01
   EXIT

IF (&PFLENT01 NE &Z AND &PFLCMD01 EQ &Z)
   &PFLCMD01 = 'SETPATH &ZPATH'

&AAA = TRUNC(&PFLENT02,' ')
IF (.TRAIL NE &Z)
   .MSG    = FLST013D
   .CURSOR = PFLENT02
   EXIT

IF (&PFLENT02 NE &Z AND &PFLCMD02 EQ &Z)
   &PFLCMD02 = 'SETPATH &ZPATH'

&AAA = TRUNC(&PFLENT03,' ')
IF (.TRAIL NE &Z)
   .MSG    = FLST013D
   .CURSOR = PFLENT03
   EXIT

IF (&PFLENT03 NE &Z AND &PFLCMD03 EQ &Z)
   &PFLCMD03 = 'SETPATH &ZPATH'

&AAA = TRUNC(&PFLENT04,' ')
IF (.TRAIL NE &Z)
   .MSG    = FLST013D
   .CURSOR = PFLENT04
   EXIT

IF (&PFLENT04 NE &Z AND &PFLCMD04 EQ &Z)
   &PFLCMD04 = 'SETPATH &ZPATH'

&AAA = TRUNC(&PFLENT05,' ')
IF (.TRAIL NE &Z)
   .MSG    = FLST013D
   .CURSOR = PFLENT05
   EXIT

IF (&PFLENT05 NE &Z AND &PFLCMD05 EQ &Z)
   &PFLCMD05 = 'SETPATH &ZPATH'

&AAA = TRUNC(&PFLENT06,' ')
IF (.TRAIL NE &Z)
   .MSG    = FLST013D
   .CURSOR = PFLENT06
   EXIT

IF (&PFLENT06 NE &Z AND &PFLCMD06 EQ &Z)
   &PFLCMD06 = 'SETPATH &ZPATH'

&AAA = TRUNC(&PFLENT07,' ')
IF (.TRAIL NE &Z)
   .MSG    = FLST013D
   .CURSOR = PFLENT07
   EXIT

IF (&PFLENT07 NE &Z AND &PFLCMD07 EQ &Z)
   &PFLCMD07 = 'SETPATH &ZPATH'

&AAA = TRUNC(&PFLENT08,' ')
IF (.TRAIL NE &Z)
   .MSG    = FLST013D
   .CURSOR = PFLENT08
   EXIT

IF (&PFLENT08 NE &Z AND &PFLCMD08 EQ &Z)
   &PFLCMD08 = 'SETPATH &ZPATH'

&AAA = TRUNC(&PFLENT09,' ')
IF (.TRAIL NE &Z)
   .MSG    = FLST013D
   .CURSOR = PFLENT09
   EXIT

IF (&PFLENT09 NE &Z AND &PFLCMD09 EQ &Z)
   &PFLCMD09 = 'SETPATH &ZPATH'

&AAA = TRUNC(&PFLENT10,' ')
IF (.TRAIL NE &Z)
   .MSG    = FLST013D
   .CURSOR = PFLENT10
   EXIT

IF (&PFLENT10 NE &Z AND &PFLCMD10 EQ &Z)
   &PFLCMD10 = 'SETPATH &ZPATH'

&AAA = TRUNC(&PFLENT11,' ')
IF (.TRAIL NE &Z)
   .MSG    = FLST013D
   .CURSOR = PFLENT11
   EXIT

IF (&PFLENT11 NE &Z AND &PFLCMD11 EQ &Z)
   &PFLCMD11 = 'SETPATH &ZPATH'

&AAA = TRUNC(&PFLENT12,' ')
IF (.TRAIL NE &Z)
   .MSG    = FLST013D
   .CURSOR = PFLENT12
   EXIT

IF (&PFLENT12 NE &Z AND &PFLCMD12 EQ &Z)
   &PFLCMD12 = 'SETPATH &ZPATH'

&AAA = TRUNC(&PFLENT13,' ')
IF (.TRAIL NE &Z)
   .MSG    = FLST013D
   .CURSOR = PFLENT13
   EXIT

IF (&PFLENT13 NE &Z AND &PFLCMD13 EQ &Z)
   &PFLCMD13 = 'SETPATH &ZPATH'

&AAA = TRUNC(&PFLENT14,' ')
IF (.TRAIL NE &Z)
   .MSG    = FLST013D
   .CURSOR = PFLENT14
   EXIT

IF (&PFLENT14 NE &Z AND &PFLCMD14 EQ &Z)
   &PFLCMD14 = 'SETPATH &ZPATH'

&AAA = TRUNC(&PFLENT15,' ')
IF (.TRAIL NE &Z)
   .MSG    = FLST013D
   .CURSOR = PFLENT15
   EXIT

IF (&PFLENT15 NE &Z AND &PFLCMD15 EQ &Z)
   &PFLCMD15 = 'SETPATH &ZPATH'

VPUT (PFLENT00,PFLCMD00) PROFILE
VPUT (PFLENT01,PFLCMD01) PROFILE
VPUT (PFLENT02,PFLCMD02) PROFILE
VPUT (PFLENT03,PFLCMD03) PROFILE
VPUT (PFLENT04,PFLCMD04) PROFILE
VPUT (PFLENT05,PFLCMD05) PROFILE
VPUT (PFLENT06,PFLCMD06) PROFILE
