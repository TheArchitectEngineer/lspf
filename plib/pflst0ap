VPUT (PFLENT13,PFLCMD13) PROFILE
VPUT (PFLENT14,PFLCMD14) PROFILE
VPUT (PFLENT15,PFLCMD15) PROFILE

)FIELD
FIELD(PFLCMD00) LEN(1024) IND(PFLC00IN)
FIELD(PFLCMD01) LEN(1024) IND(PFLC01IN)
FIELD(PFLCMD02) LEN(1024) IND(PFLC02IN)
FIELD(PFLCMD03) LEN(1024) IND(PFLC03IN)
FIELD(PFLCMD04) LEN(1024) IND(PFLC04IN)
FIELD(PFLCMD05) LEN(1024) IND(PFLC05IN)
FIELD(PFLCMD06) LEN(1024) IND(PFLC06IN)
FIELD(PFLCMD07) LEN(1024) IND(PFLC07IN)
FIELD(PFLCMD08) LEN(1024) IND(PFLC08IN)
FIELD(PFLCMD09) LEN(1024) IND(PFLC09IN)
FIELD(PFLCMD10) LEN(1024) IND(PFLC10IN)
FIELD(PFLCMD11) LEN(1024) IND(PFLC11IN)
FIELD(PFLCMD12) LEN(1024) IND(PFLC12IN)
FIELD(PFLCMD13) LEN(1024) IND(PFLC13IN)
FIELD(PFLCMD14) LEN(1024) IND(PFLC14IN)

)END
