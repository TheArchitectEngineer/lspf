)PANEL VERSION=1 FORMAT=1
)COMMENT
 Keylist Utilit Panel 5
)ENDCOMMENT

)BODY WINDOW(60,11)

PanelTitle "Create Keylist Table"

text     2   2         FP  "Command ===> "
field    2  15  MAX    NEF CAPS(ON),PAD(USER) ZCMD

text     4    2   FP    "New Table Name . . . . . ."
field    4   29  4     NEF CAPS(ON),PAD(USER) NEWTAB

text    6 2  CH "Instructions"
text    8 2  NT "Press"
text    8 8  ET "Enter"
text    8 14 NT "to confirm"

text    9 2  NT "Press"
text    9 8  ET "Cancel"
text    9 15 NT "or"
text    9 18 ET "END"
text    9 22 NT "to cancel"

)INIT
.cursor  = newtab
&NEWTAB  = &Z
&ZWINTTL = &Z

)PROC
&ZCMD = TRANS( &ZCMD CAN,CANCEL *,* )

IF ( &ZCMD = CANCEL )
   .RESP = END
   EXIT
ELSE
   IF (&ZCMD NE &Z )
      .MSG    = PSYS018
      .CURSOR = ZCMD
      EXIT

VER (&NEWTAB,NB)

)END
