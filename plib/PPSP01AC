)PANEL VERSION=1 FORMAT=1
)COMMENT
 Browse entry panel
)ENDCOMMENT

)BODY

PanelTitle "Display Command Tables"

scrollOn

literal  4   2         FP    "Command ===> "  
field    4  15  MAX-17 NEF CAPS(ON) ZCMD  

literal  4  MAX-15  FP "SCROLL ===> "
field    4  MAX-3  4 NEF  NONE ZSCROLL

literal  6  2      FP    "Profile:"  
field    6  11    8 VOI  NONE ZAPPLID

literal  6  MAX-38     FP  "Application Command Table.:"  
field    6  MAX-10  8  VOI  NONE   APPLCMD  

literal  7  MAX-38     FP  "User Command Table . . . .:"  
literal  7  MAX-10     VOI  "USRCMDS"  

literal  8  MAX-38     FP   "System Command Table . . .:"  
literal  8  MAX-10     VOI  "ISPCMDS"  

literal  7  2    FP    "Command Table:"  
field    7  17   8      NEF CAPS(ON) CMDTAB

field    8  2   50 VOI  NONE APPLCMDL

literal  10  2      CH    "Command   Trunc Action                                             Description"  
literal  11  2      CH    EXPAND "-"  

tbmodel  12   MAX
tbfield  2    8    VOI   NONE ZCTVERB
tbfield  ++2  1    VOI   NONE ZCTTRUNC
tbfield  +6   50   VOI   NONE ZCTACT
tbfield  ++1  MAX  VOI   NONE ZCTDESC

)INCLUDE STD_PDC
)INCLUDE HLP_PDC

)INIT
VGET ZSCROLL PROFILE
.ATTR(ZCTVERB) ='COLOUR(YELLOW)'
.ATTR(ZCTTRUNC)='COLOUR(WHITE)'
.ATTR(ZCTDESC) ='COLOUR(WHITE)'

)PROC

IF (&ZCMD EQ ADD )
   .MSG    = PSYS015
   .CURSOR = ZCMD
   EXIT
ELSE
   IF (&ZCMD NE &Z)
      .MSG    = PSYS01C
      .CURSOR = ZCMD
      EXIT

VPUT ZSCROLL PROFILE


)END
