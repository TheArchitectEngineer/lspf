&INTENS31 = TRANS(TRUNC(&INTENS31,1) H,HIGH L,LOW &Z,&Z MSG=PPSP017)
&HILITE31 = TRANS(TRUNC(&HILITE31,1) N,NONE B,BLINK R,REVERSE, U,USCORE &Z,&Z MSG=PPSP018)

&COLOUR32 = TRANS(TRUNC(&COLOUR32,1) R,RED G,GREEN Y,YELLOW B,BLUE M,MAGENTA P,MAGENTA T,TURQ W,WHITE &Z,&Z MSG=PPSP016)
&INTENS32 = TRANS(TRUNC(&INTENS32,1) H,HIGH L,LOW &Z,&Z MSG=PPSP017)
&HILITE32 = TRANS(TRUNC(&HILITE32,1) N,NONE B,BLINK R,REVERSE, U,USCORE &Z,&Z MSG=PPSP018)

&COLOUR33 = TRANS(TRUNC(&COLOUR33,1) R,RED G,GREEN Y,YELLOW B,BLUE M,MAGENTA P,MAGENTA T,TURQ W,WHITE &Z,&Z MSG=PPSP016)
&INTENS33 = TRANS(TRUNC(&INTENS33,1) H,HIGH L,LOW &Z,&Z MSG=PPSP017)
&HILITE33 = TRANS(TRUNC(&HILITE33,1) N,NONE B,BLINK R,REVERSE, U,USCORE &Z,&Z MSG=PPSP018)

&COLOUR34 = TRANS(TRUNC(&COLOUR34,1) R,RED G,GREEN Y,YELLOW B,BLUE M,MAGENTA P,MAGENTA T,TURQ W,WHITE &Z,&Z MSG=PPSP016)
&INTENS34 = TRANS(TRUNC(&INTENS34,1) H,HIGH L,LOW &Z,&Z MSG=PPSP017)
&HILITE34 = TRANS(TRUNC(&HILITE34,1) N,NONE B,BLINK R,REVERSE, U,USCORE &Z,&Z MSG=PPSP018)

)PNTS
FIELD(ZPS00001) VAR(ZCMD) VAL(DEFAULTS)
FIELD(ZPS00002) VAR(ZCMD) VAL(CANCEL)
FIELD(ZPS00003) VAR(ZCMD) VAL(SAVE)
FIELD(ZPS00004) VAR(ZCMD) VAL(RESTORE)

)END
