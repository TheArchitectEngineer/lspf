IF (.MSG EQ &Z,PPSP011B) REFRESH(*)
ELSE                     REFRESH(ZCMD)

)PROC
IF (&ZCMD EQ CANCEL)
   .RESP = END
   EXIT

VGET ZVERB SHARED
IF (&ZVERB EQ CANCEL)
   &ZCMD = CANCEL
   EXIT

&ZCMD = TRANS( &ZCMD DEF,DEFAULTS
                     DEFAULTS,DEFAULTS
                     RES,RESTORE
                     REST,RESTORE
                     RESTORE,RESTORE
                     SAV,SAVE
                     SAVE,SAVE
                     &Z,&Z
                     MSG=PPSP015
             )

IF (&ZCMD EQ DEFAULTS,RESTORE) EXIT

